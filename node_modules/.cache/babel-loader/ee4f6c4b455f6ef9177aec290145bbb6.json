{"ast":null,"code":"import axios from \"axios\";\nexport default {\n  name: \"MyPage\",\n\n  data() {\n    return {\n      tot_asset: 0,\n      // Ïù¥Í±¥ Í≥ÑÏÇ∞ ÏïåÏïÑÏÑú Ìï†Í≤åÏöî\n      // Ïù¥ Ï†ïÎ≥¥Î•º ÌéòÏù¥ÏßÄ Î°úÎìúÏãúÏóê ÏöîÏ≤≠ÌïòÏó¨ Î∞õÏïÑÏò¨ Í≤ÉÏûÑ\n      own_money: {\n        money: 0\n      },\n      own_stock_list: [],\n      group_stock_list: [],\n      selectedStock: 0\n    };\n  },\n\n  mounted() {\n    axios.get('/api/user/stock/own').then(response => {\n      this.own_stock_list = response.data.own_stock_list;\n      this.own_money = response.data.own_money;\n      this.tot_asset = response.data.own_money;\n\n      for (let i = 0; i < this.own_stock_list.length; i++) {\n        this.tot_asset += this.own_stock_list[i].stock_count * this.own_stock_list[i].price;\n      }\n\n      this.own_stock_list.map(stock => {\n        stock.total_price = String(stock.stock_count * stock.price).replace(/\\B(?=(\\d{3})+(?!\\d))/g, \",\");\n      });\n      this.tot_asset = String(this.tot_asset).replace(/\\B(?<!\\.\\d*)(?=(\\d{3})+(?!\\d))/g, \",\");\n      this.own_money = String(this.own_money).replace(/\\B(?<!\\.\\d*)(?=(\\d{3})+(?!\\d))/g, \",\");\n      this.own_stock_list.map(stock => {\n        stock.price = String(stock.price).replace(/\\B(?<!\\.\\d*)(?=(\\d{3})+(?!\\d))/g, \",\");\n      });\n      this.$refs.loading1.style.display = 'none';\n    }).catch(error => {\n      console.log(error);\n    });\n    this.getMyStockList();\n  },\n\n  methods: {\n    removeFromMyStock() {\n      console.log(this.selectedStock);\n      axios.delete('/api/user/stock/group/' + this.selectedStock).then(() => {\n        alert('Í¥ÄÏã¨Ï¢ÖÎ™©ÏóêÏÑú ÏÇ≠Ï†ú ÎêòÏóàÏäµÎãàÎã§.');\n        this.$refs.loading2.style.display = 'block';\n        this.getMyStockList();\n        this.$refs.loading2.style.display = 'none';\n      });\n    },\n\n    getMyStockList() {\n      axios.get('/api/user/stock/group/list').then(response => {\n        this.group_stock_list = response.data;\n        this.group_stock_list.map(stock => {\n          stock.price = String(stock.price).replace(/\\B(?<!\\.\\d*)(?=(\\d{3})+(?!\\d))/g, \",\");\n          stock.volume = String(stock.volume).replace(/\\B(?<!\\.\\d*)(?=(\\d{3})+(?!\\d))/g, \",\");\n        });\n        this.$refs.loading2.style.display = 'none';\n      }).catch(error => {\n        console.log(error);\n      });\n    },\n\n    radioChange(event) {\n      this.selectedStock = event.target.value;\n      console.log(event.target.value);\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAkQA;AAEA;EACAA,cADA;;EAGAC;IACA;MACAC,YADA;MACA;MACA;MACAC;QACAC;MADA,CAHA;MAOAC,kBAPA;MASAC,oBATA;MAWAC;IAXA;EAaA,CAjBA;;EAmBAC;IACAC,iCACAC,IADA,CACAC;MACA;MACA;MAEA;;MACA;QACA;MACA;;MACA;QACAC;MACA,CAFA;MAIA;MACA;MACA;QACAA;MACA,CAFA;MAIA;IACA,CApBA,EAqBAC,KArBA,CAqBAC;MACAC;IACA,CAvBA;IAyBA;EACA,CA9CA;;EAgDAC;IACAC;MACAF;MACAN,4DACAC,IADA,CACA;QACAQ;QACA;QACA;QACA;MACA,CANA;IAOA,CAVA;;IAYAC;MACAV,wCACAC,IADA,CACAC;QACA;QACA;UACAC;UACAA;QACA,CAHA;QAIA;MACA,CARA,EASAC,KATA,CASAC;QACAC;MACA,CAXA;IAYA,CAzBA;;IA2BAK;MACA;MACAL;IACA;;EA9BA;AAhDA","names":["name","data","tot_asset","own_money","money","own_stock_list","group_stock_list","selectedStock","mounted","axios","then","response","stock","catch","error","console","methods","removeFromMyStock","alert","getMyStockList","radioChange"],"sourceRoot":"src/components","sources":["MyPage.vue"],"sourcesContent":["<template>\n  <div class=\"max-w-[72rem] mx-auto\">\n    <div class=\"text-gray-900 dark:text-gray-300 mb-5 pb-5\n\t\t\t\t\t\tborder-gray-600 border-b \">\n      <p class=\"text-3xl font-bold\">\n        Ï¥ù ÌèâÍ∞ÄÏûêÏÇ∞ : {{ tot_asset }}Ïõê üòé\n      </p>\n      <span class=\"text-gray-600 dark:text-gray-400\">\n\t\t\t\tÏòàÏπòÍ∏à : {{ own_money }}Ïõê\n\t\t\t</span>\n    </div>\n    <div class=\"grid lg:grid-cols-2 gap-6\">\n      <div id=\"own-stock-list\"\n           class=\"p-4 bg-white border rounded-lg sm:p-8 dark:bg-gray-800 dark:border-gray-700 flex flex-col ga