{
  "name": "tvjs-overlays",
  "version": "0.5.0",
  "description": "Overlay pack for trading-vue-js",
  "main": "./dist/tvjs-overlays.js",
  "scripts": {
    "name-check": "node ./webpack/overlay-parser.js --silent",
    "check": "node ./webpack/overlay-parser.js",
    "compile": "node ./webpack/index-compiler.js",
    "compile-prod": "node ./webpack/index-compiler.js --prod",
    "eslint": "./node_modules/.bin/eslint --ext .js,.vue src",
    "dev": "webpack serve --config webpack/dev.config.js --mode development --progress --hot --port=$PORT",
    "build": "webpack  --config webpack/build.config.js --mode production --progress"
  },
  "author": "C451",
  "license": "MIT",
  "files": [
    "dist/*",
    "src/*"
  ],
  "keywords": [
    "trading",
    "trading-vue-js",
    "tvjs",
    "overlays",
    "crypto"
  ],
  "homepage": "https://github.com/tvjsx/trading-vue-js#readme",
  "repository": "github:tvjsx/tvjs-overlays",
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/plugin-transform-runtime": "^7.14.5",
    "@babel/preset-env": "^7.14.7",
    "babel-loader": "^8.2.2",
    "colors": "^1.4.0",
    "css-loader": "^3.6.0",
    "eslint": "^7.30.0",
    "eslint-config-eslint": "^7.0.0",
    "eslint-plugin-vue": "^7.13.0",
    "html-webpack-plugin": "^5.3.2",
    "minimist": "^1.2.5",
    "node-fetch": "^2.6.1",
    "puppeteer": "^10.1.0",
    "recursive-readdir": "^2.2.2",
    "recursive-readdir-sync": "^1.0.6",
    "request": "^2.88.2",
    "terser-webpack-plugin": "^5.1.4",
    "tulind": "^0.8.18",
    "vue-loader": "^15.9.6",
    "vue-style-loader": "^4.1.3",
    "vue-template-compiler": "^2.6.14",
    "webpack": "^5.43.0",
    "webpack-cli": "^4.7.2",
    "webpack-dev-server": "^3.11.2"
  },
  "dependencies": {
    "@babel/runtime": "^7.14.6",
    "@vue/compiler-sfc": "^3.1.4",
    "trading-vue-js": "^1.0.2",
    "vue": "^2.6.14"
  }
}
